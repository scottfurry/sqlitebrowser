{
  "format": {
    "line_width": 96,
    "tab_size": 4,
    "max_subgroups_hwrap": 3,
    "max_pargs_hwrap": 3,
    "separate_ctrl_name_with_space": false,
    "separate_fn_name_with_space": false,
    "dangle_parens": true,
    "line_ending": "unix",
    "command_case": "upper",
    "keyword_case": "upper"
  },
  "encode": {
    "emit_byteorder_mark": false
  }
}
